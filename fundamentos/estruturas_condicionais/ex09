<body>
    <script>
        var n = parseInt(prompt('Digite um número qualquer:'), 10);

        if(n % 5 === 0){
            alert(`O número ${n} é múltiplo de 5.`);
        }else{
            alert(`O número ${n} não é múltiplo de 5.`)
        }
    </script>
</body>

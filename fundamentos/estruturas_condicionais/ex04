<body>
    <script>
        var n1 = parseInt(prompt('Digite um número qualquer:'), 10);
        var n2 = parseInt(prompt('Digite um número qualquer:'), 10);

        if(n1 + n2 >= 10){
            alert(`A soma de ${n1} e ${n2} é igual a ${n1 + n2}`);
        }
    </script>
</body>

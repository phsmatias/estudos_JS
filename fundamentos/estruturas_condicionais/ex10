<body>
    <script>
        var n = parseInt(prompt('Digite um número qualquer:'), 10);

        if(20 < n && n < 90){
            alert(`O número ${n} está entre 20 e 90.`)
        }else{
            alert(`O número ${n} não está entre 20 e 90.`)
        }
    </script>
</body>

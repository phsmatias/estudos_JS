    <script>
        var a = parseInt(prompt('Digite o valor da variável A:'), 10);
        var b = parseInt(prompt('Digite o valor da variável B:'), 10);

        var c = a;
        var a = b;
        var b = c;

        alert(`O valor de B:${b} e o valor de A:${a}`);
    </script>

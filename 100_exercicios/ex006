<body>
    <script>
        //ENUNCIADO: Ler dois números inteiros e exibir o quociente e o resto da divisão inteira entre eles.

        var n1 = parseInt(prompt('Digite o primeiro número inteiro:'), 10);
        var n2 = parseInt(prompt('Digite o segundo número inteiro:'), 10);

        console.log(`Quociente: ${n1/n2}`);
        console.log(`Resto: ${n1%n2}`);
    </script>
</body>
